{
  "name": "mooncode",
  "displayName": "MoonCode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:Mooncode.activate"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "MoonCode.showInitialLanguagesData",
        "title": "MoonCode: Show Initial Languages Data",
        "icon": "e"
      },
      {
        "command": "MoonCode.showCurrentLanguagesData",
        "title": "MoonCode: Show Current Languages Data",
        "icon": "e"
      },
      {
        "command": "MoonCode.showInitialFilesData",
        "title": "MoonCode: Show Initial Files Data",
        "icon": "e"
      },
      {
        "command": "MoonCode.showCurrentFilesData",
        "title": "MoonCode: Show Current Files Data",
        "icon": "e"
      },
      {
        "command": "MoonCode.login",
        "title": "MoonCode: Login",
        "icon": "lol"
      },
      {
        "command": "MoonCode.register",
        "title": "MoonCode: Register",
        "icon": "lol"
      },
      {
        "command": "MoonCode.logout",
        "title": "MoonCode: Logout",
        "icon": "lol"
      },
      {
        "command": "MoonCode.openDashboard",
        "title": "MoonCode: Open Dashboard",
        "icon": "lol"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "build": "tsc --build",
    "watch": "webpack --watch --mode development",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@repo/trpc": "*",
    "@repo/utils": "*",
    "@trpc/client": "^11.1.0",
    "@trpc/server": "^11.1.0",
    "zod": "^3.24.2"
  }
}
