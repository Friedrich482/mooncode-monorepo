{
  "name": "@repo/common",
  "version": "0.0.0",
  "private": "true",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "lint": "eslint ."
  },
  "files": [
    "./dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "devDependencies": {
    "@repo/typescript-config": "*",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "date-fns": "^4.1.0"
  }
}
